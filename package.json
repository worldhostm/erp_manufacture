{
  "name": "erp-manufacture",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:full": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "server:dev": "cd server && npm run dev",
    "test:server": "cd server && npm test",
    "build:all": "npm run build && cd server && npm run build",
    "start:prod": "concurrently \"npm start\" \"npm run server:start\"",
    "server:start": "cd server && npm start",
    "kill-ports": "node -e \"const { exec } = require('child_process'); const isWin = process.platform === 'win32'; const cmd = isWin ? 'kill-ports-simple.bat' : './kill-ports-simple.sh'; exec(cmd, (err, stdout) => { if (err) console.error(err); else console.log(stdout); });\""
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "date-fns": "^2.30.0",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.4"
  }
}
